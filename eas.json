{
  "cli": {
    "version": ">= 14.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "ENVFILE": ".env.production"
      }
    },
    "preview2": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "preview3": {
      "developmentClient": true
    },
    "preview4": {
      "distribution": "internal"
    },
    "production": {
      "env": {
        "ENVFILE": ".env.production"
      }
    },
    "localapp": {
      "env": {
        "ANDROID_HOME": "/home/shrayank/Android",
        "ENVFILE": ".env.production"
      },
      "android": {
        "buildType": "apk"
      }
    }
  }
}
