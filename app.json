{
  "expo": {
    "name": "Breeze",
    "slug": "breeze",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "breeze",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/wind-icon-cropped.png",
    "splash": {
      "image": "./assets/images/wind.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/wind-icon-cropped.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET"
      ],
      "package": "com.breeze"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/wind-icon-cropped.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4601c3a6-8c1e-4ea2-a828-7b7ff3866d5b"
      }
    }
  }
}
